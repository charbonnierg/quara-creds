{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".venv/": true,
        "**/__pycache__": true,
    },
    "python.linting.enabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.ignorePatterns": [
        "**/*_pb2.py"
    ],
    "python.formatting.provider": "black",
}